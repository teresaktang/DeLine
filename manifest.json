{
    "manifest_version":3,
    "version":"1.0",
    "name":"Deline",
    "description": "A dictionary extension",
    "permissions": [""],
    "background": {
        "service_worker": "main.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts":[
        {
            "matches":["<all_urls>"],
            "js":["deline.js"]
        }
    ]
}