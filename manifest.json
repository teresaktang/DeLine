{
    "manifest_version":2,
    "version":"1.0",
    "name":"Deline",
    "description": "A dictionary extension",
    "permissions": [""],
    "action": {
        "default_popup": "dictionary"
    },
    "content_scripts":[
        {
            "matches":["<all_urls>"],
            "js":["main.js"]
        }
    ]
}